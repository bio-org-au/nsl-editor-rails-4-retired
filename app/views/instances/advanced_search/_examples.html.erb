<h5 class="">Count instances</h5>
<table class="example-searches table table-striped">
  <% [{search_target: 'Instances', search_string: "count",explanation: %Q(Start with "count instance" to count instances.)},
      {search_target: 'Instances', search_string: "count acacia", explanation: %Q(Counting without listing should be faster than listing for large sets.)},
      {search_target: 'Instances', search_string: "count type:basionym", explanation: %Q(Count by type e.g. basionyms.)},
      {search_target: 'Instances', search_string: "count type:comb. nov.", explanation: %Q(Count comb. novs.)},
      {search_target: 'Instances', search_string: "count acacia type:basionym", explanation: %Q(Count by name and type e.g. basionyms.)},
